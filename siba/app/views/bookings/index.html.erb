<p id="notice"><%= notice %></p>

<h1>Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Details</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Company</th>
      <th>Event type</th>
      <th>Date</th>
      <th>Bar type</th>
      <th>Add ons</th>
      <th>Address</th>
      <th>Guests</th>
      <th>Start time</th>
      <th>Permit</th>
      <th>Additional</th>
      <th>Details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.details %></td>
        <td><%= booking.name %></td>
        <td><%= booking.email %></td>
        <td><%= booking.phone %></td>
        <td><%= booking.company %></td>
        <td><%= booking.event_type %></td>
        <td><%= booking.date %></td>
        <td><%= booking.bar_type %></td>
        <td><%= booking.add_ons %></td>
        <td><%= booking.address %></td>
        <td><%= booking.guests %></td>
        <td><%= booking.start_time %></td>
        <td><%= booking.permit %></td>
        <td><%= booking.additional %></td>
        <td><%= booking.details %></td>
        <td><%= link_to 'Show', booking %></td>
        <td><%= link_to 'Edit', edit_booking_path(booking) %></td>
        <td><%= link_to 'Destroy', booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Booking', new_booking_path %>
